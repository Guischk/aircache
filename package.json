{
  "name": "aircache",
  "version": "0.1.0",
  "description": "High-performance Airtable cache service with REST API and dual-namespace Redis strategy",
  "author": "Guischk",
  "license": "MIT",
  "keywords": ["airtable", "cache", "redis", "api", "bun", "typescript"],
  "main": "index.ts",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun index.ts",
    "dev": "bun --hot index.ts",
    "build": "bun run airtable:types && bun build index.ts --target=bun --outdir=dist",
    "build:railway": "bun install && bun run build",
    "test": "bun test tests/",
    "test:api": "bun test tests/api.test.ts",
    "test:integration": "bun tests/integration.test.ts",
    "test:performance": "bun tests/performance.test.ts",
    "test:all": "bun scripts/run-tests.ts",
    "benchmark:redis-vs-airtable": "bun tests/redis-vs-airtable.benchmark.ts",
    "benchmark:sqlite-vs-redis": "bun tests/sqlite-vs-redis.benchmark.ts",
    "benchmark:sqlite-vs-airtable": "bun tests/sqlite-vs-airtable.benchmark.ts",
    "test:deployment": "bun scripts/test-sqlite-deployment.ts",
    "security-check": "bun scripts/security-check.ts",
    "demo": "bun scripts/demo.ts",
    "airtable:types": "airtable-types-gen --output src/lib/airtable/schema.ts -f",
    "clean": "rm -rf dist *.md *-report.md *-comparison.md"
  },
  "exports": {
    ".": "./index.ts",
    "./api": "./src/api/index.ts",
    "./server": "./src/server/index.ts",
    "./redis": "./src/lib/redis/index.ts",
    "./sqlite": "./src/lib/sqlite/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "airtable": "^0.12.2",
    "airtable-types-gen": "^0.4.0"
  }
}
